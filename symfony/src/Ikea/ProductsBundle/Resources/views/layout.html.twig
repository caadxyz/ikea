<!DOCTYPE html>
<html class="no-js">

    <head>
        <title>{% block title %}IKEA products{% endblock %}</title>
        <!-- Bootstrap -->
        <link href="{{ asset('bootstrap/bootstrap/css/bootstrap.css') }}" rel="stylesheet" media="screen">
        <link href="{{ asset('bootstrap/assets/styles.css') }}" rel="stylesheet" media="screen">
        <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
        <!--[if lt IE 9]>
            <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
    </head>

    <body>
        <div class="navbar navbar-fixed-top">
            <div class="navbar-inner">
                <div class="container-fluid">
                    <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse"> <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </a>
                    <a class="brand" href="{{ path('ikea_products_homepage') }}">IKEA</a>
                    <!--/.nav-collapse -->
                </div>
            </div>
        </div>
        <div class="container-fluid" style="padding-top:5px;">
            <div class="row-fluid">
                <!--/span-->
                <div class="span10" id="content">  
                       {% block wrapper %}{% endblock %}
                    </div>
                </div>
                <hr>
                <footer>
                    <p>&copy; IKEA</p>
                </footer>
            </div>
        </body>

    </html>
